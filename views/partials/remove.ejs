<div class="mt-4 p-5 jumbotron">
    <h2>Supprimer un item</h2>
    <form id="removebyid">
        <input type="number" name="id" placeholder="ID de l'item" class="form-control">
        <button type="submit" class="btn btn-danger mt-2">Supprimer</button>
    </form>
    <form id="removebyname">
        <input type="text" name="name" placeholder="Nom de l'item" class="form-control mt-2">
        <button type="submit" class="btn btn-danger mt-2">Supprimer</button>
    </form>
</div>